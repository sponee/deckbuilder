<div class = "well">
   <%= form_for [@xml_file], html: { multipart: true } do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
   <p><%= link_to "Cancel", xml_files_path(current_user) %>
</div>